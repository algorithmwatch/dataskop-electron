{
  "name": "dataskop-electron",
  "version": "0.4.12",
  "description": "DataSkop Electron App: The Personal Data Dashboard (PeDD) of DataSkop",
  "scripts": {
    "build": "concurrently \"npm run build:main\" \"npm run build:renderer\"",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.prod.ts",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir src",
    "lint": "cross-env NODE_ENV=development eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "package": "ts-node ./.erb/scripts/clean.js dist && npm run build && electron-builder build --publish never",
    "package-windows": "ts-node ./.erb/scripts/clean.js dist && npm run build && electron-builder build --publish never --win",
    "postinstall": "ts-node .erb/scripts/check-native-dep.js && electron-builder install-app-deps && cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.erb/configs/webpack.config.renderer.dev.dll.ts",
    "start": "ts-node ./.erb/scripts/check-port-in-use.js && npm run start:renderer",
    "start:main": "cross-env NODE_ENV=development electron -r ts-node/register/transpile-only  ./src/main/main.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.erb/configs/webpack.config.renderer.dev.ts",
    "test": "jest",
    "cross-env": "cross-env",
    "format": "prettier --write .",
    "schema": "./scripts/create_schema.sh",
    "version:pre": "cd release/app && npm version prerelease --no-git-tag-version --preid beta && cd ../.. && npm version prerelease --no-git-tag-version --preid beta",
    "version:preminor": "cd release/app && npm version preminor --no-git-tag-version --preid beta && cd ../.. && npm version preminor --no-git-tag-version --preid beta",
    "version:prepatch": "cd release/app && npm version prepatch --no-git-tag-version --preid beta && cd ../.. && npm version prepatch --no-git-tag-version --preid beta",
    "version:minor": "cd release/app && npm version minor --no-git-tag-version && cd ../.. && npm version minor --no-git-tag-version",
    "version:patch": "cd release/app && npm version patch --no-git-tag-version && cd ../.. && npm version patch --no-git-tag-version",
    "postversion": "./scripts/postversion.sh $npm_package_version",
    "delete-old-releases": "ts-node ./scripts/delete_old_releases.ts",
    "audit": "./scripts/npm_audit.sh"
  },
  "build": {
    "productName": "DataSkop",
    "appId": "org.algorithmwatch.DataSkop",
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "files": [
      "dist",
      "node_modules",
      "package.json"
    ],
    "afterSign": ".erb/scripts/Notarize.js",
    "mac": {
      "target": {
        "target": "default",
        "arch": "universal"
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "extraResources": [
      "./assets/**"
    ],
    "generateUpdatesFilesForAllChannels": true,
    "publish": {
      "provider": "s3",
      "region": "de",
      "endpoint": "https://s3.de.cloud.ovh.net",
      "bucket": "dataskop"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/algorithmwatch/dataskop-electron.git"
  },
  "author": {
    "name": "AW AlgorithmWatch gGmbH",
    "email": "it@algorithmwatch.org",
    "url": "https://algorithmwatch.org"
  },
  "contributors": [
    {
      "name": "Johannes Filter",
      "email": "hi@jfilter.de",
      "url": "https://johannesfilter.com"
    },
    {
      "name": "Gregor Weichbrodt",
      "email": "gregorweichbrodt@gmail.com",
      "url": "https://gregorweichbrodt.de"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/algorithmwatch/dataskop-electron/issues"
  },
  "keywords": [
    "electron",
    "dataskop"
  ],
  "homepage": "https://dataskop.net",
  "jest": {
    "testURL": "http://localhost/",
    "testEnvironment": "jsdom",
    "transform": {
      "\\.(ts|tsx|js|jsx)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(lowdb|steno|p-queue|p-timeout|d3-array|internmap|d3-force|d3-quadtree|d3-dispatch|d3-timer|d3-selection|@observablehq/plot|d3|delaunator|robust-predicates|isoformat))"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.erb/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "^renderer/(.*)": "<rootDir>/src/renderer/$1",
      "^renderer": "<rootDir>/src/renderer",
      "lowdb": "lowdb/lib/index"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "release/app/node_modules"
    ],
    "testPathIgnorePatterns": [
      "release/app/dist"
    ],
    "setupFiles": [
      "./.erb/scripts/check-build-exists.ts"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    }
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.95.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.1",
    "@teamsupercell/typings-for-css-modules-loader": "^2.4.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/archiver": "^5.3.1",
    "@types/base-64": "^1.0.0",
    "@types/cheerio": "^0.22.29",
    "@types/glidejs__glide": "^3.4.1",
    "@types/history": "4.7.9",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.168",
    "@types/node": "16.10.2",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/react-modal": "^3.13.1",
    "@types/react-router": "^5.1.16",
    "@types/react-router-dom": "^5.3.0",
    "@types/react-table": "^7.0.29",
    "@types/react-test-renderer": "^17.0.1",
    "@types/react-window": "^1.8.5",
    "@types/terser-webpack-plugin": "^5.0.4",
    "@types/uuid": "^8.3.0",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "autoprefixer": "^10.2.5",
    "browserslist-config-erb": "^0.0.3",
    "chalk": "^4.1.2",
    "concurrently": "^6.3.0",
    "core-js": "^3.18.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.3.0",
    "css-minimizer-webpack-plugin": "^3.1.0",
    "detect-port": "^1.3.0",
    "dotenv-webpack": "^7.0.3",
    "electron": "^19.0.10",
    "electron-builder": "^23.3.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-notarize": "^1.1.1",
    "electron-rebuild": "^3.2.3",
    "eslint": "^7.32.0",
    "eslint-config-erb": "^4.0.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.2.4",
    "mini-css-extract-plugin": "^2.3.0",
    "postcss": "^8.2.9",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.7.1",
    "react-refresh": "^0.10.0",
    "react-refresh-typescript": "^2.0.2",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "tailwindcss": "^2.2.19",
    "terser-webpack-plugin": "^5.2.4",
    "ts-jest": "^27.0.5",
    "ts-json-schema-generator": "^0.97.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.56.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "@algorithmwatch/harke": "^0.4.8",
    "@fortawesome/fontawesome-pro": "^5.15.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/pro-duotone-svg-icons": "^5.15.4",
    "@fortawesome/pro-light-svg-icons": "^5.15.4",
    "@fortawesome/pro-regular-svg-icons": "^5.15.4",
    "@fortawesome/pro-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@glidejs/glide": "^3.5.2",
    "@headlessui/react": "^1.6.6",
    "@material-ui/core": "^4.12.4",
    "@observablehq/plot": "^0.5.2",
    "@sentry/electron": "^3.0.7",
    "@tippyjs/react": "^4.2.6",
    "ajv": "^8.11.0",
    "archiver": "^5.3.1",
    "base-64": "^1.0.0",
    "chrono-node": "^2.3.9",
    "classnames": "^2.3.1",
    "d3-array": "^3.1.6",
    "d3-force": "^3.0.0",
    "d3-selection": "^3.0.0",
    "dayjs": "^1.11.4",
    "electron-debug": "^3.2.0",
    "electron-fetch": "^1.7.4",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.2.1",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "lowdb": "^3.0.0",
    "p-limit": "3",
    "p-queue": "^7.3.0",
    "perfect-arrows": "^0.3.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.38.0",
    "react-json-view": "^1.21.3",
    "react-modal": "^3.15.1",
    "react-rnd": "^10.3.7",
    "react-router-dom": "^5.3.1",
    "react-use": "^17.4.0",
    "react-window": "^1.8.7",
    "regenerator-runtime": "^0.13.9",
    "source-map-support": "^0.5.21",
    "uuid": "^8.3.1"
  },
  "devEngines": {
    "node": ">=16.x",
    "npm": ">=7.x"
  },
  "browserslist": [],
  "prettier": {
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".babelrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ],
    "singleQuote": true,
    "trailingComma": "all"
  }
}
